#!/usr/bin/env python3
from typing_extensions import Type
import argparse
import sys 
def iniciar():

    parser = argparse.ArgumentParser(description='Parámetros de SecondPip')

    parser.add_argument(
        '-n', 
        type=int,
        default=5,
        required=True,
        action='store',
        help='número de valores de z para cada solución',
    )
    
    parser.add_argument(
        '-N', 
        type= int,
        default=100000,
        required=False,
        help='número de vectores l y k',
    )
    
    parser.add_argument(
        '-m', 
        type=int,
        default=6,
        required=False,
        help='máximo (mínimo) valor que l (k) puede tomar',
    )
   
    parser.add_argument(
        '-zmax', 
        type=int,
        default=30,
        required=False,
        action='store'
        help='máximo (mínimo) valor que l (k) puede tomar',
    )
    
    parser.add_argument(
        '-imax', 
        type=int,
        default=0, 
        required=False, 
        action='store',
        help='valor máximo de iteraciones para hallar las soluciones'
    )
    

    args = parser.parse_args()
    return args

def main(args): 
  
  if args.n:
      a=args.number*2
  return  print(a) 

  if args.N:
      a=args.number*3
  return  print(a) 

  if args.m:
      a=args.number*4
  return  print(a) 

  if args.zmax:
      a=args.number*5
  return  print(a) 

  if args.imax:
      a=args.number*6
  return  print(a) 

 
if __name__ == '__main__': 
    args=iniciar()
    main(args)
    
   
