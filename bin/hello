from desoper import SecondPip
from multiprocessing import Pool

import argparse
parser = argparse.ArgumentParser(
          description='Exclamative salutation to the World')
parser.add_argument('n',         
                    action='store_true',
                    help='Input a n entero')
                    
args = parser.parse_args()                    
               
if __name__ == '__main__':
  s = SecondPip.all_sol_for_n(n)
  if args.n:
      n = int(n)  
  print(s)
  

#n = int(input("Por favor ingrese el valor de n que sea mayor que 4; "))
resultados = Pool().map(all_sol_for_n, list(range(n,n+1)))

df=pd.DataFrame(resultados)
df=df.T
df=df.astype(str)
df.drop_duplicates().reset_index(drop=True)
