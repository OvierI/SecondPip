#!/usr/bin/env python3

import argparse
import sys 
def iniciar():

    parser = argparse.ArgumentParser(description='argumentos de pedida del programa')

    parser.add_argument(
        'n', 
        'number',
        type=int,
        default=5,
        required=True,
        action='store',
        help='número de valores de z para cada solución',
    )
    
    parser.add_argument(
        '-N', 
        '--numlist',
        type= int,
        required=False,
        help='número de vectores l y k',
    )
    
    parser.add_argument(
        '-m', 
        '--magnitud',
        type=int,
        default=1,
        required=False,
        help='máximo (mínimo) valor que l (k) puede tomar',
    )
   
    parser.add_argument(
        '-zmax', 
        '--zmaximo',
        type=int,
        required=False
        actio='store'
        help='máximo (mínimo) valor que l (k) puede tomar',
    )
    
    parser.add_argument(
        '-imax', 
        '--imaximo',
        type=int,
        default=0, 
        required=False, 
        help='valor máximo de iteraciones para hallar las soluciones'
    )
    

    args = parser.parse_args()
    

def main(args): 
  
  if args.number:
      a=args.number*3
  return  print(a) 
 
if __name__ == '__main__': 
    inputs = {
        "n": 5,
        "m": 6,
        "N": 500000,
        "zmax": 30,
        "imax": 0,
    }
