#!/usr/bin/env python3
#from typing_extensions import Type
import argparse
import sys 
def iniciar():

    parser = argparse.ArgumentParser(description='Parámetros de SecondPip')

    parser.add_argument(
        '-n', 
        type=int,
        default=5,
        required=True,
        action='store',
        help='número de valores de z para cada solución',
    )
    
    parser.add_argument(
        '-N', 
        type= int,
        default=100000,
        required=True,
        action='store',
        help='número de vectores l y k',
    )
    
    parser.add_argument(
        '-m', 
        type=int,
        default=6,
        required=True,
        action='store',
        help='máximo (mínimo) valor que l (k) puede tomar',
    )
   
    parser.add_argument(
        '-zmax', 
        type=int,
        default=30,
        required=True,
        action='store',
        help='máximo (mínimo) valor que z puede tomar',
    )
    
    parser.add_argument(
        '-imax', 
        type=int,
        default=0, 
        required=True, 
        action='store',
        help='valor máximo de iteraciones para hallar las soluciones'
    )
    

    args = parser.parse_args()
    return args

def main(args): 
  
  if args.n:
      a=args.n*2
  #return  print(a) 

  if args.N:
      b=args.N*3
  #return  print(b) 

  if args.m:
      c=args.m*4
  #return  print(c) 

  if args.zmax:
      d=args.zmax*5
  #return  print(d) 

  if args.imax:
      e=args.imax*6
  return  print(a,b,c,d,e) 

 
if __name__ == '__main__': 
    args=iniciar()
    main(args)
    
   
